        <script type="text/javascript">
            let currtimg = null;
            let bttnhover = null;
            window.addEventListener("load", () => {
                let bttnAll = document.querySelectorAll("#bttn-img");
                bttnAll[0].classList.toggle("active");
                let firstimg = document.querySelectorAll(".fix-consultation-img")[0];
                firstimg.classList.toggle("visible");
                currtimg = firstimg;
                bttnhover = bttnAll[0];




            })

            let bttnAll = document.querySelectorAll("#bttn-img");


            bttnAll.forEach(bttn => {
                bttn.addEventListener("click", () => {
                    if (bttnhover != null) {
                        bttnhover.classList.toggle("active");
                    }
                    bttn.classList.toggle("active");

                    let bttnimg = bttn.getAttribute("data-img");

                    document.querySelectorAll(".fix-consultation-img").forEach(img => {
                        let imgAttr = img.getAttribute("data-img");
                        if (imgAttr == bttnimg) {
                            if (currtimg != null) {
                                currtimg.classList.toggle("visible");

                            }

                            img.classList.toggle("visible");
                            currtimg = img;
                            bttnhover = bttn;
                        }

                    })
                }

                )

            })


        </script>
