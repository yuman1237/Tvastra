        </section>
        <section class="send-message-section">
            <ul>
                <li><i class="fas fa-home"></i>
                    <h4>Noida,India</h4>
                    <p>Sector 45</p>
                </li>
                <li><i class="fas fa-phone-alt"></i>
                    <h4>00(440)986 562</h4>
                    <p>Man to Fri 9am to 6pm</p>
                </li>

                <li><i class="fas fa-home"></i>
                    <h4>support@tvstra.com</h4>
                    <p>Send us your query anytime!</p>
                </li>

            </ul>
            <form action="submit">
                <input type="text" class="name" placeholder="Enter your Name">
                <input type="text" class="address" placeholder="Enter email address">
                <input type="text" class="subject" placeholder="Enter subject">
                <input type="text" class="message" placeholder="Enter Message">
                <button class="secondary-bttn-light">Send Message</button>
            </form>
        </section>
    </main>
